<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

	<package name="mypack" extends="struts-default" namespace="/">

		<action name="index" class="pack1.Login">
			<result name="success">/home.jsp</result>
			<result name="input">/index.jsp</result>
		</action>

		<action name="addmovie" class="pack1.AddMovie">
			<result name="success">/MovieSuccessful.jsp</result>
			<result name="input">/AddMovie.jsp</result>
		</action>

		<action name="delete" class="pack1.DeleteMovie">
			<result name="success">/home.jsp</result>

		</action>
		<action name="list" class="pack1.List">
			<result name="success">/List.jsp</result>

		</action>
		
		 <action name="logout" class="pack1.Logout">
			<result name="success">/index.jsp</result>
			
		</action>

	</package>


</struts>